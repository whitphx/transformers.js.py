{
  "name": "pyodide-e2e",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:front": "vite",
    "dev:python": "chokidar \"../transformers_js_py/**/*.py\" \"../transformers_js/**/*.py\" --initial -c \"cd .. && poetry build\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@vitest/browser": "^1.2.2",
    "@xenova/transformers": "^2.17.2",
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "pyodide": "^0.26.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^1.2.2",
    "webdriverio": "^8.39.1"
  }
}
