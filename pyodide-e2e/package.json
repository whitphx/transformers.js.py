{
  "name": "pyodide-e2e",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:front": "vite",
    "dev:python": "chokidar \"../src/transformers_js_py/**/*.py\" \"../src/transformers_js/**/*.py\" --initial -c \"cd .. && uv build\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@vitest/browser": "^2.1.9",
    "@xenova/transformers": "^2.17.2",
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.51.1",
    "prettier": "3.5.3",
    "pyodide": "^0.27.6",
    "typescript": "^5.7.2",
    "vite": "^6.2.2",
    "vitest": "^2.1.9"
  }
}
